{
  "phpschemamanagertest": {
    "book": {
      "columns": {
        "id": {
          "type": "serial",
          "size": "10",
          "allownull": "no",
          "defaultvalue": ""
        },
        "title": {
          "type": "varchar",
          "size": "222",
          "allownull": "no",
          "defaultvalue": ""
        },
        "isbn": {
          "type": "char",
          "size": "11",
          "allownull": "no",
          "defaultvalue": ""
        },
        "language": {
          "type": "char",
          "size": "3",
          "allownull": "no",
          "defaultvalue": "en"
        },
        "summary": {
          "type": "text",
          "size": "0",
          "allownull": "yes",
          "defaultvalue": "null"
        }
      },

      "keys": {
        "isbnidx": {
          "type": "regular",
          "columns": ["isbn"]
        },
        "wrongidx": {
          "type": "regular",
          "columns": ["title"]
        },
        "multidx": {
          "type": "regular",
          "columns": ["title", "language"]
        },
        "titleidx": {
          "type": "regular",
          "columns": ["title"]
        },
        "primary": {
          "type": "pk",
          "columns": ["id"]
        }
      }
    },

    "wrongtable": {
      "columns": {
        "id": {
          "type": "serial",
          "size": "10",
          "allownull": "no",
          "defaultvalue": ""
        },
        "wrongage": {
          "type": "int",
          "size": "4",
          "allownull": "yes",
          "defaultvalue": "null"
        }
      },

      "keys": {
        "primary": {
          "type": "pk",
          "columns": ["id"]
        }
      }
    }

  }
}